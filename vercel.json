{
  "version": 2,
  "builds": [
    {
      "src": "api/recipe.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/recipe.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}